	issn = {0027-8424, 1091-6490},
	journaltitle = {Proc. Natl. Acad. Sci. U.S.A.},
	langid = {english},
	number = {34},
	pages = {13995--13998},
	title = {Carbonaceous meteorites contain a wide range of extraterrestrial nucleobases},
	url = {https://pnas.org/doi/full/10.1073/pnas.1106493108},
	urldate = {2025-01-11},
